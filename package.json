    "clsx": "^2.1.1",
    "kysely": "^0.28.11",
    "kysely-d1": "^0.4.0",
    "lucide-react": "^0.575.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-fate": "^0.1.3",
    "react-hook-form": "^7.71.2",
    "react-server-dom-webpack": "19.2.4",
